try:

    def rectangle_area(w, h):
        result = w * h
        return result


    print("Для нахождения площади четвероугольника введите его ширину и длину")
    a = float(input("Введите ширину четвероугольника "))
    b = float(input("Введите длину четвероугольника "))
    c = rectangle_area(a, b)
    print("Площадь четвероугольника равна", c)

except ValueError:
    print("Введенное значение не было числом/не могло быть воспринято программой как число.")